<window>
    <defaultcontrol>100</defaultcontrol>
	<coordinates>
		<system>1</system>
		<posx>161</posx>
		<posy>100</posy>
	</coordinates>
	<controls>
		<!-- ** Optional ** -->
		<control type="image">
			<description>Background Image</description>
			<posx>-49</posx>
			<posy>18</posy>
			<width>500</width>
			<height>380</height>
			<texture>showtimes-dialog-panel.png</texture>
		</control>
		<!-- ** Required ** Do not change <description> or <type> -->
		<control type="label">
			<description>Showtimes Label1</description>
			<posx>-30</posx>
			<posy>28</posy>
			<width>350</width>
			<align>left</align>
			<font>font12</font>
            <textcolor>FFFFFFFF</textcolor>
            <label>$INFO[Container(50).Property(Title)]</label>
		</control>
		<!-- ** Required ** Do not change <description> or <type> -->
		<control type="label">
			<description>Showtimes Label2</description>
			<posx>-30</posx>
			<posy>50</posy>
			<width>450</width>
			<align>left</align>
			<font>font10</font>
			<textcolor>DDced8da</textcolor>
            <label>$INFO[Container(50).Property(Location)]$INFO[Container(50).Property(Phone), - ,]</label>
		</control>
		<!-- ** Required ** Do not change <description> or <type> -->
		<control type="label">
			<description>Showtimes Label3</description>
			<posx>-30</posx>
			<posy>72</posy>
			<width>450</width>
			<align>left</align>
			<font>font10</font>
			<textcolor>DDced8da</textcolor>
            <label>$INFO[Container(50).Property(Date)]</label>
		</control>
		<!-- ** Required ** Do not change <description> or <type> -->
		<control type="label">
			<description>Showtimes Label4</description>
			<posx>440</posx>
			<posy>28</posy>
			<width>200</width>
			<align>right</align>
			<font>font10</font>
			<textcolor>DDced8da</textcolor>
            <label>$INFO[Container(50).Property(Locale)]</label>
		</control>
		<!-- ** Required ** Do not change <id> or <type> -->
		<control type="list" id="50">
			<description>List</description>
			<posx>-30</posx>
			<posy>100</posy>
			<width>470</width>
			<height>260</height>
			<scrolltime>200</scrolltime>
            <onup>100</onup>
            <ondown>100</ondown>
			<itemlayout height="65">
				<control type="image">
					<width>470</width>
					<height>62</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture border="5">showtimes-input-nofocus.png</texture>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>1</posy>
					<width>310</width>
					<height>18</height>
					<font>font10</font>
                    <textcolor>FFFFFFFF</textcolor>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>465</posx>
					<posy>1</posy>
					<width>150</width>
					<height>18</height>
					<font>font10</font>
                    <textcolor>FFFFFFFF</textcolor>
					<aligny>center</aligny>
					<align>right</align>
					<info>ListItem.Property(Phone)</info>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>21</posy>
					<width>460</width>
					<height>18</height>
					<font>font10</font>
                    <textcolor>FFFFFFFF</textcolor>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Property(Address)</info>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>41</posy>
					<width>460</width>
					<height>18</height>
					<font>font10</font>
                    <textcolor>60FFFFFF</textcolor>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Property(ShowTimes)</info>
				</control>
			</itemlayout>
			<focusedlayout height="65">
				<control type="image">
					<width>470</width>
					<height>62</height>
					<posx>0</posx>
					<posy>0</posy>
					<texture border="5">showtimes-input-focus.png</texture>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>1</posy>
					<width>310</width>
					<height>18</height>
					<font>font10</font>
                    <textcolor>FFFFFFFF</textcolor>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
				<control type="label">
					<posx>465</posx>
					<posy>1</posy>
					<width>150</width>
					<height>18</height>
					<font>font10</font>
                    <textcolor>FFFFFFFF</textcolor>
					<aligny>center</aligny>
					<align>right</align>
					<info>ListItem.Property(Phone)</info>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>21</posy>
					<width>460</width>
					<height>18</height>
					<font>font10</font>
                    <textcolor>FFFFFFFF</textcolor>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Property(Address)</info>
				</control>
				<control type="label">
					<posx>5</posx>
					<posy>41</posy>
					<width>460</width>
					<height>18</height>
					<font>font10</font>
                    <textcolor>60FFFFFF</textcolor>
					<aligny>center</aligny>
					<align>left</align>
					<info>ListItem.Property(ShowTimes)</info>
				</control>
			</focusedlayout>
		</control>
	</controls>
</window>