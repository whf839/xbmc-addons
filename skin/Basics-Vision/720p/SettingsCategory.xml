<window id="12">
	<defaultcontrol>3</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<include>Common_Background</include>
		<control type="group">
			<animation effect="slide" time="150" start="-345,0">WindowOpen</animation>
			<animation effect="slide" time="150" end="-345,0">WindowClose</animation>
			<control type="image">
				<posx>-20</posx>
				<posy>0</posy>
				<width>400</width>
				<height>720</height>
				<onclick>-</onclick>
				<texture>Side_Slideout.png</texture>
			</control>
			<control type="group">
				<posx>0</posx>
				<posy>110</posy>
				<control type="grouplist" id="3">
					<description>button area</description>
					<posx>0</posx>
					<posy>30</posy>
					<width>340</width>
					<height>455</height>
					<itemgap>5</itemgap>
					<onleft>5</onleft>
					<onright>5</onright>
					<onup>3</onup>
					<ondown>3</ondown>
				</control>
			</control>
		</control>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>90</height>
			<texture>Topbar.png</texture>
		</control>
		<control type="label">
			<description>Title label</description>
			<posx>40</posx>
			<posy>20</posy>
			<width>600</width>
			<height>42</height>
			<align>left</align>
			<aligny>center</aligny>
			<textcolor>white</textcolor>
			<shadowcolor>black</shadowcolor>
			<label>5</label>
			<font>font16b</font>
		</control>
		<control type="label" id="2">
			<description>Title label2</description>
			<posx>250</posx>
			<posy>20</posy>
			<width>600</width>
			<height>42</height>
			<align>left</align>
			<aligny>center</aligny>
			<textcolor>white</textcolor>
			<shadowcolor>black</shadowcolor>
			<label>5</label>
			<font>font16b</font>
		</control>
		<include>Common_Now_Playing</include>
		<control type="grouplist" id="5">
			<description>control area</description>
			<posx>330</posx>
			<posy>110</posy>
			<width>930</width>
			<height>500</height>
			<itemgap>5</itemgap>
			<pagecontrol>90</pagecontrol>
			<onleft>3</onleft>
			<onright>90</onright>
			<onup>5</onup>
			<ondown>5</ondown>
			<include>Window_OpenClose_Animation</include>
		</control>
		<control type="group">
			<include>Window_OpenClose_Animation</include>
			<posx>300r</posx>
			<posy>650</posy>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>300</width>
				<height>44</height>
				<texture>Slider_focus_left.png</texture>
			</control>
			<control type="spincontrol" id="90">
				<description>Next page button</description>
				<posx>220</posx>
				<posy>10</posy>
				<subtype>page</subtype>
				<font>font13</font>
				<onleft>5</onleft>
				<onright>3</onright>
				<ondown>90</ondown>
				<onup>90</onup>
				<textcolor>red</textcolor>
				<showonepage>true</showonepage>
			</control>
		</control>
		<control type="button" id="10">
			<description>Default Category Button</description>
			<posx>0</posx>
			<posy>0</posy>
			<include>ButtonCommonValues</include>
			<label>-</label>
			<align>left</align>
		</control>
		<control type="button" id="7">
			<description>Default Button</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<font>font13</font>
			<textcolor>white</textcolor>
			<textoffsetx>20</textoffsetx>
			<texturefocus>ListItem_Focus.png</texturefocus>
			<texturenofocus>-</texturenofocus>
			<pulseonselect>no</pulseonselect>
		</control>
		<control type="radiobutton" id="8">
			<description>Default RadioButton</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<font>font13</font>
			<textcolor>white</textcolor>
			<texturefocus>ListItem_Focus.png</texturefocus>
			<texturenofocus>-</texturenofocus>
			<textoffsetx>20</textoffsetx>
			<textureradiofocus>radiobutton-focus.png</textureradiofocus>
			<textureradionofocus>radiobutton-nofocus.png</textureradionofocus>
			<radioposx>890</radioposx>
			<pulseonselect>no</pulseonselect>
		</control>
		<control type="spincontrolex" id="9">
			<description>Default SpinControlex</description>
			<posx>0</posx>
			<posy>0</posy>
			<height>45</height>
			<texturefocus>ListItem_Focus.png</texturefocus>
			<texturenofocus>-</texturenofocus>
			<textureup>scroll-up.png</textureup>
			<texturedown>scroll-down.png</texturedown>
			<textureupfocus>scroll-up-focus.png</textureupfocus>
			<texturedownfocus>scroll-down-focus.png</texturedownfocus>
			<font>font13</font>
			<textcolor>white</textcolor>
			<disabledcolor>disabled</disabledcolor>
			<textoffsetx>20</textoffsetx>
			<spinposx>20</spinposx>
			<aligny>center</aligny>
			<reverse>yes</reverse>
			<pulseonselect>no</pulseonselect>
		</control>
	</controls>
</window>