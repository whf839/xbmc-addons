			<posx>121</posx>
			<posy>40</posy>
			<width>411</width>
			<height>160</height>
			<onleft>503</onleft>
			<onright>503</onright>
			<onup>503</onup>
			<ondown>503</ondown>
			<pagecontrol>-</pagecontrol>
			<scrolltime>200</scrolltime>
			<itemlayout height="40">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>411</width>
					<height>36</height>
					<texture>dialog-button-nofocus.png</texture>
				</control>
				<control type="label">
					<posx>8</posx>
					<posy>4</posy>
					<width>394</width>
					<height>28</height>
					<font>font13</font>
					<aligny>center</aligny>
					<selectedcolor>red</selectedcolor>
					<align>left</align>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="40">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>411</width>
					<height>36</height>
					<visible>!Control.HasFocus(503)</visible>
					<texture>dialog-button-nofocus.png</texture>
				</control>
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>433</width>
					<height>36</height>
